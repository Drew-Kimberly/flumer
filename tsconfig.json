{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "esModuleInterop": true,
    "noImplicitAny": false
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/**/__tests__",
    "src/**/__mocks__",
    "src/**/__fixtures__",
    "build"
  ]
}
